p = 0
u = 0
t = 0
l = 0
def circucle1():
    global p
    fill(250,255,18)
    ellipse(960,540,p,p)
    p = p + 1.5
def circucle2():
    global u
    fill(255)
    ellipse(960,540,u,u)
    u = u + 1.5
def circucle3():
    global t
    fill(20,232,2)
    ellipse(960,540,t,t)
    t = t + 1.5
def circucle2():
    global l
    fill(255)
    ellipse(960,540,l,l)
    l = l + 1.5


def setup():
    fullScreen()
    noStroke()
def draw():
    global p, u, t, l
    circucle1()
    circucle2()
    
